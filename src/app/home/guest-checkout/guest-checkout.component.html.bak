          </ng-container> <br>

          <div class="col-sm-12 text-center free-ship-tag">
            FREE SHIPPING TO ALL USA ADDRESSES
          </div>
          
          <div class="row">
            <div class="col-xs-12 ">
              <img width="100%" src="assets/images/shipping/security.jpg" alt="security">
              <!-- <img src="assets/images/shipping/security.jpg" alt="security">
              <img src="assets/images/shipping/security.jpg" alt="security"> -->
            </div>
  
            <div class="col-xs-6">
              <img width="100%" src="assets/images/shipping/getgeotrustsslseal.png" alt="security">
            </div>
            <div class="col-xs-6">
              <img width="100%" src="assets/images/shipping/Credit_Card_Safe_light.png" alt="security"></div>
          </div>
         <div class="col-sm-12 text-success text-center">All information is encrypted and transmitted without riskusing a Secure Socket Layer (SSL) protocol.</div> <hr>

         <div style="padding-left: 0px;padding-right: 0px;" class="col-sm-12">
           <h3 style="font-weight: normal;color: #000;">REVIEW YOUR ORDER</h3>

           <div id="product" class="row items" *ngFor="let item of Items; index as i">
            <ng-container >
              <div class="col-xs-6">
                <img *ngIf="!item.plateText1" style="cursor: pointer;" (click)="getProduct(item)" class="card-img-top" width="100%" src="{{imgUrl}}{{item.productImage}}" alt="IMG_PRODUCT"> <br>
                <img *ngIf="item.plateText1" class="card-img-top" width="90%" src="https://www.licenseplates.tv/lpgenI.php?productId={{item.productModel}}&text1={{item.plateText1}}&text2={{item.plateText2}}&font={{font}}" alt="IMG_PRODUCT">
                <!-- <div class="row">
                  <div style="padding-left: 0px;padding-right: 0px" class="col-xs-6 text-left">
                    <h6 style="cursor: pointer;" (click)="edit(item,i);"><u>Edit</u> </h6>
                  </div>
                  <div style="padding-left: 0px;padding-right: 0px" class="col-xs-6 text-right">
                    <h6 style="cursor: pointer;" (click)="remove(i);"><u>Remove</u>  </h6>
                  </div>
                </div> -->
              </div>
      
              <div class="col-xs-6">
                <h6 style="width: 100%;margin: auto;color: #000" class="card-title">{{item.productName}}</h6> 
                <div class="row">
                  <div style="padding-left: 0px;padding-right: 0px;margin-top: 2px" class="col-xs-12">
                    <h6 *ngIf="item.plateText1">Text: <span class="a6x12mun">{{item.plateText1}}</span> </h6> 
                    <h6 *ngIf="item.plateText2">Text 2: <span class="a6x12mun">{{item.plateText2}}</span></h6>
                    <h6 class="text-danger"><strong>${{item.TotalPrice | number : '1.2-2'}}</strong> </h6>
                  </div>
                  <div style="padding-left: 0px;padding-right: 0px;" *ngIf="item.plateType == 'Add Black Plate Holder'" class="col-xs-12">
                      <h6><small>Universal Plate Holder - {{item.plateType}}</small> </h6>
                    <img width="100" src="assets/images/universalplateholder1.jpg" alt="IMG_Black Plate Holder">
                  </div>
                  <div style="padding-left: 0px;padding-right: 0px;" *ngIf="item.plateType == 'Add CHROME Plate Holder'" class="col-xs-12">
                      <h6><small>Universal Plate Holder - {{item.plateType}}</small> </h6>
                      <img width="100" src="assets/images/universalplateholder2.jpg" alt="IMG_Black Plate Holder">
                    </div>
                  <div id="tile_div" style="padding-left: 0px;padding-right: 0px;margin-top: 5px" class="col-xs-12 text-center">
                    <!-- <div class="col-xs-6 col-sm-6 text-center"> -->
                      <!-- <span class="tile_div">
                        <button (click)="decriment(item)" class="btn"><i class="fa fa-minus" aria-hidden="true"></i></button>&nbsp;
                       &nbsp;<h5 class="txtnum">{{item.itemQuantity}}</h5>&nbsp;
                       <button (click)="incriment(item)" class="btn"><i class="fa fa-plus" aria-hidden="true"></i></button>&nbsp;
                      </span>                -->
                     <!-- </div> -->
                  </div>
                </div>
              </div>
            </ng-container>

            

          </div> <!--Row-->

          <div class="col-sm-12">
            <div class="form-group">
              <label style="font-weight: normal; color: #000;" for="exampleFormControlTextarea1">SPECIAL INSTRUCTIONS OR ORDER COMMENTS (optional)</label>
              <textarea style="height: auto;" [(ngModel)]="model.instructions" #ctrl="ngModel" class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
            </div>
          </div>

          <div id="checkout-box" style="background: #ddd;" class="row">
            <div style="padding-left: 0px;padding-right: 0px" class="col-xs-12 table-responsive">

                <table *ngIf="sum" class="table">
                    <thead>
                      <tr>
                        <th scope="col">Coupon:</th>

                        <th scope="col">
                            <input type="text" [(ngModel)]="model.coupon" #ctrl="ngModel" class="form-control ">
                        </th>

                        <th scope="col"><button (click)="checkCoupon()" class="btn btn-danger">apply</button></th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td scope="col">Sub-Total:</td>
                        <td></td>
                        <td><strong>${{sum | number : '1.2-2'}}</strong></td>
                      </tr>
                      <tr>
                        <td scope="col">Tax:</td>
                        <td></td>
                        <td><strong>${{tax | number : '1.2-2'}}</strong></td>
                      </tr>
                      <tr>
                        <td scope="col">Coupon Discount:</td>
                        <td></td>
                        <td><strong>${{coupon | number : '1.2-2'}}</strong></td>
                      </tr>
                      <tr class="border-bot">
                        <td>Shipping:</td> 
                        <td></td>
                        <td *ngIf="ship_amt == 0" class="text-danger">FREE</td>
                        <td *ngIf="ship_amt > 0" class="text-secondary">${{ship_amt | number : '1.2-2'}}</td>
                      </tr>
                      <tr>
                          <td>Total:</td>
                          <td></td>
                          <!-- <td><strong>$ {{getTotalAmount()}}</strong></td> -->
                           <td><strong>${{totalamount | number : '1.2-2'}}</strong></td>
                        </tr>
                    </tbody>
                  </table>

            </div>
            
          </div>

         </div>

    </div>

    <div class="col-sm-12"> <br>
      <h3>PAYMENT METHOD</h3>
